<!-- ----------------------------------- MODALS ------------------------------------ -->
<!--	LOGIN MODAL -->
<div id="modal-login" class="login modal grey lighten-3">
	<nav class="login-nav valign-wrapper grey lighten-2">
			<h6 class="center-align grey-text text-darken-1">Login with e-UP Account</h6>
	</nav>
	<div class="login-div container">
		<div class="login-form">
			<form action="landing.html">
				<div class="input-field">
					<i class="login-icon material-icons prefix">email</i>
          <input id="email" type="email" class="validate">
          <label for="email">Email (@up.edu.ph)</label>
				</div>
				<div class="input-field">
					<i class="login-icon material-icons prefix">lock</i>
          <input id="password" type="password" class="validate">
          <label for="password">Password</label>
				</div>
				<div class="input-field row">
          <button class="blue lighten-1 large btn waves-effect waves-light center-align col s8 offset-s2" type="submit" name="action">Login
					<i class="material-icons right">send</i></button>
				</div>
			</form>
		</div>
	</div>
	<div class="login-footer grey lighten-2 valign-wrapper">
			<h6 class="center-align grey-text text-darken-1">Find out how to get your e-UP account <a href="https://www.uplbosa.org">here</a></h6>
	</div>
</div>

<!-- FEEDBACK MODAL	-->
<div id="modal-feedback" class="feedback modal grey lighten-3">
	<nav class="feedback-nav valign-wrapper grey lighten-2">
			<h6 class="center-align grey-text text-darken-1">Curriculum Feedback</h6>
	</nav>
	<div class="feedback-div container">
		<div class="feedback-form">
			<form>
				<div class="feedback-sem">
					<h6 class="blue-text text-lighten-1">Preferred Semester</h6>
					<p class="range-field"><input type="range" min="0" max="11" /></p>
				</div>
        <div class="feedback-comment input-field">
					<h6 class="blue-text text-lighten-1">Comments</h6>
          <textarea id="comments" class="materialize-textarea grey lighten-2"></textarea>
        </div>
				<div class="input-field row">
          <button class="blue lighten-1 large btn waves-effect waves-light center-align col s8 offset-s2" type="submit" name="action" onClick="switchModal()">Submit
					<i class="material-icons right">send</i></button>
				</div>
			</form>
		</div>
	</div>
</div>
<!-- CURRICULUM MODAL -->
<div id="modal-curriculum" class="curriculum modal grey lighten-3">
	<nav class="curriculum-nav valign-wrapper grey lighten-2">
			<h6 class="center-align grey-text text-darken-1">Create New Curriculum</h6>
	</nav>
	<div class="login-div container">
		<div class="login-form">
			<form action="canvas.html">
				<div class="input-field">
					<i class="login-icon material-icons prefix">email</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Degree Program</label>
				</div>
				<div class="input-field">
					<i class="login-icon material-icons prefix">lock</i>
          <input id="icon_prefix" type="number" class="validate">
					<label for="icon_prefix">Years</label>
				</div>
				<div class="input-field row">
          <button class="blue lighten-1 large btn waves-effect waves-light center-align col s8 offset-s2" type="submit" name="action">Create
					<i class="material-icons right">send</i></button>
				</div>
			</form>
		</div>
	</div>
</div>
